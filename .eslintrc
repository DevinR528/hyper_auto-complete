{
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "no-underscore-dangle": "off",
      "no-console":"off",
      "no-plusplus":"off",
      "no-unused-vars":"off",
      "no-control-regex":"off",
      "class-methods-use-this": "off",
      "brace-style": "off",
      "for-direction": "off",
      "no-undef": "off",
      "no-multiple-empty-lines": "off"
    }
  }